<body>
  <div class="container">
    <h1 class="mt-4">Coupen Management</h1>

    <table class="table mt-4">
      <thead class="thead-dark">
        <tr>
          <th>Coupen Name</th>
          <th>Expiry Date</th>
          <th>Minimum Price</th>
          <th>Quantity</th>
          <th>Max Discount</th>
          <th>Status</th>
          <th>Actions</th>
        </tr>
      </thead>
      <tbody>

      
        <% coupen.forEach((coupen) => { %>
          <tr>
            <td><%= coupen.coupenName %></td>
            <td><%= coupen.expiryDate.toLocaleDateString() %></td>
            <td><%= coupen.minimumPrice %></td>
            <td><%= coupen.quantity %></td>
            <td><%= coupen.discount %>%</td>
            <td><%= coupen.status %></td>
            <td>
              <a href="/admin/editCoupen/<%= coupen._id %>" class="btn btn-primary">Edit</a>

              
                <a href="/admin/deleteCoupen/<%=coupen._id%>"><button type="submit" class="btn btn-danger">Delete</button></a>
            </td>
          </tr>
        <% }) %>
        
       
      </tbody>

    </table>
    <a href="/admin/addCoupen" class="btn btn-primary" style="margin-left: 395px;">Add Coupon</a>

  </div>

